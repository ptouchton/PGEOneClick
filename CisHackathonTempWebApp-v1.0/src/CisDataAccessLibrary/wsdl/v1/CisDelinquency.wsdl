      <wsdl:input message="bons0:getDelinquencyRulesRequestMsg" name="getDelinquencyRulesRequest"/>
      <wsdl:output message="bons0:getDelinquencyRulesResponseMsg" name="getDelinquencyRulesResponse"/>
