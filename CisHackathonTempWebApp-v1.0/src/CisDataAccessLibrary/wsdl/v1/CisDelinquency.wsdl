            <xsd:element name="getDelinquencyRules">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="delinquentRuleCode" nillable="true" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="getDelinquencyRulesResponse">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="delinquencyRules" nillable="true" type="bons0:DelinquencyRulesDto"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="getDelinquencyRules_recordNotFound" type="bons4:RecordNotFoundFault"/>
      <xsd:element name="getDelinquencyRules_serviceFault" type="bons4:ServiceFault"/>
            <xsd:element name="getDelinquencyNotice">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="accountNumber" nillable="true" type="bons3:AccountNumberDto"/>
