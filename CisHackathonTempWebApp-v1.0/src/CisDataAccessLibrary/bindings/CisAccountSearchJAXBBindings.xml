<jaxws:bindings
	wsdlLocation="file:../wsdl/v1/CisAccountSearch.wsdl"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:jaxws="http://java.sun.com/xml/ns/jaxws"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb">
	
	<!-- customize the generated fault class names -->
	
	<!-- accountSearch -->
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisAccountSearch']/wsdl:operation[@name='accountSearch']/wsdl:fault[@name='accountNotFound']">
		<jaxws:class name="AccountNotFoundMsg"/>
	</jaxws:bindings>
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisAccountSearch']/wsdl:operation[@name='accountSearch']/wsdl:fault[@name='serviceFault']">
		<jaxws:class name="ServiceFaultMsg"/>
	</jaxws:bindings>
			
</jaxws:bindings>
