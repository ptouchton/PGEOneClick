<jaxws:bindings
	wsdlLocation="file:../wsdl/v1/CisPremise.wsdl"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:jaxws="http://java.sun.com/xml/ns/jaxws"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb">
	
	<!-- customize the generated fault class names -->
	
	<!-- getPremiseInfo -->
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisPremise']/wsdl:operation[@name='getPremiseInfo']/wsdl:fault[@name='premiseNotFound']">
		<jaxws:class name="PremiseNotFoundMsg"/>
	</jaxws:bindings>
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisPremise']/wsdl:operation[@name='getPremiseInfo']/wsdl:fault[@name='serviceFault']">
		<jaxws:class name="ServiceFaultMsg"/>
	</jaxws:bindings>
		
	<!-- createPremise -->
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisPremise']/wsdl:operation[@name='createPremise']/wsdl:fault[@name='validationFault']">
		<jaxws:class name="ValidationFaultMsg"/>
	</jaxws:bindings>
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisPremise']/wsdl:operation[@name='createPremise']/wsdl:fault[@name='serviceFault']">
		<jaxws:class name="ServiceFaultMsg"/>
	</jaxws:bindings>	
</jaxws:bindings>
