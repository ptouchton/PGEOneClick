	</jaxws:bindings>
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisNote']/wsdl:operation[@name='getTodaysNotes']/wsdl:fault[@name='serviceFault']">
		<jaxws:class name="ServiceFaultMsg"/>
	</jaxws:bindings>
	
</jaxws:bindings>
