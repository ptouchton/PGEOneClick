	</jaxws:bindings>
	
	<!-- getNotes -->
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisNote']/wsdl:operation[@name='getNotes']/wsdl:fault[@name='notesNotFound']">
		<jaxws:class name="NotesNotFoundMsg"/>
	</jaxws:bindings>
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisNote']/wsdl:operation[@name='getNotes']/wsdl:fault[@name='serviceFault']">
		<jaxws:class name="ServiceFaultMsg"/>
