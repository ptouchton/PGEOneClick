	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb">
	
	<!-- customize the generated fault class names -->
	
	<!-- createNotes -->
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisNote']/wsdl:operation[@name='createNotes']/wsdl:fault[@name='serviceFault']">
		<jaxws:class name="ServiceFaultMsg"/>
	</jaxws:bindings>
		
	<!-- createNotesByPremise -->
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisNote']/wsdl:operation[@name='createNotesByPremise']/wsdl:fault[@name='serviceFault']">
