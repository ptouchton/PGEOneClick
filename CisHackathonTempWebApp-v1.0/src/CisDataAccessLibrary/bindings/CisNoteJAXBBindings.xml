		<jaxws:class name="ServiceFaultMsg"/>
	</jaxws:bindings>
	
	<!-- getTodaysNotes -->
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisNote']/wsdl:operation[@name='getTodaysNotes']/wsdl:fault[@name='notesNotFound']">
