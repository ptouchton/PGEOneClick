<jaxws:bindings
	wsdlLocation="file:../wsdl/v1/CisBilling.wsdl"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:jaxws="http://java.sun.com/xml/ns/jaxws"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb">
	
	<!-- customize the generated fault class names -->
	
	<!-- getBillingCycleDueDayRanges -->	
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisBilling']/wsdl:operation[@name='getBillingCycleDueDayRanges']/wsdl:fault[@name='serviceFault']">
		<jaxws:class name="ServiceFaultMsg"/>
	</jaxws:bindings>
			
	<!-- getBillingCalendar -->	
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisBilling']/wsdl:operation[@name='getBillingCalendar']/wsdl:fault[@name='serviceFault']">
		<jaxws:class name="ServiceFaultMsg"/>
	</jaxws:bindings>
	
	<!-- getLastBillDate -->	
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisBilling']/wsdl:operation[@name='getLastBillDate']/wsdl:fault[@name='billingNotFound']">
		<jaxws:class name="BillingNotFoundMsg"/>
	</jaxws:bindings>
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisBilling']/wsdl:operation[@name='getLastBillDate']/wsdl:fault[@name='serviceFault']">
		<jaxws:class name="ServiceFaultMsg"/>
	</jaxws:bindings>
	
</jaxws:bindings>
