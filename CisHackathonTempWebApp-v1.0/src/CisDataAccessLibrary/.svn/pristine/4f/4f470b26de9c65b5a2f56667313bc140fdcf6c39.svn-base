<jaxws:bindings
	wsdlLocation="file:../wsdl/v1/CisPaymentExtension.wsdl"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:jaxws="http://java.sun.com/xml/ns/jaxws"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb">
	
	<!-- customize the generated fault class names -->
	
	<!-- createPaymentExtension -->
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisPaymentExtension']/wsdl:operation[@name='createPaymentExtension']/wsdl:fault[@name='serviceFault']">
		<jaxws:class name="ServiceFaultMsg"/>
	</jaxws:bindings>
		
	<!-- updatePaymentExtension -->
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisPaymentExtension']/wsdl:operation[@name='updatePaymentExtension']/wsdl:fault[@name='recordNotFoundFault']">
		<jaxws:class name="RecordNotFoundMsg"/>
	</jaxws:bindings>
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisPaymentExtension']/wsdl:operation[@name='updatePaymentExtension']/wsdl:fault[@name='serviceFault']">
		<jaxws:class name="ServiceFaultMsg"/>
	</jaxws:bindings>
	
	<!-- getActivePaymentExtension -->
	<jaxws:bindings node="wsdl:definitions/wsdl:portType[@name='CisPaymentExtension']/wsdl:operation[@name='getActivePaymentExtension']/wsdl:fault[@name='serviceFault']">
		<jaxws:class name="ServiceFaultMsg"/>
	</jaxws:bindings>
</jaxws:bindings>
