<?xml version="1.0" encoding="UTF-8"?><xsd:schema targetNamespace="http://www.pgn.com/2010/07/cis/xsd" xmlns:bons0="http://www.pgn.com/2010/07/cis/xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:include schemaLocation="BillingCycleDueDayRangeDto.xsd"/>
    <xsd:complexType name="BillingCycleDueDayRangesDto">
    <xsd:sequence>
            <xsd:element minOccurs="0" name="minDate" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The earliest date from the database  that was used to derive the cycle ranges</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
            <xsd:element minOccurs="0" name="maxDate" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The last date from the database  that was used to derive the cycle ranges</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="billingCycleDueDayRange" type="bons0:BillingCycleDueDayRangeDto">
        <xsd:annotation>
          <xsd:documentation>See BillingCycleDayRangeDto</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
        </xsd:sequence>
  </xsd:complexType>
</xsd:schema>