<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.pgn.com/2011/07/cis/xsd"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:complexType name="InstallmentDto">
		<xsd:sequence>
			<xsd:element minOccurs="1" name="amount"
				type="xsd:double">
			</xsd:element>
			<xsd:element minOccurs="0" name="chargeDate"
				type="xsd:dateTime">
			</xsd:element>
			<xsd:element minOccurs="0" name="dueDate"
				type="xsd:dateTime">
			</xsd:element>
			<xsd:element minOccurs="0" name="paidInFullDate"
				type="xsd:dateTime">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>